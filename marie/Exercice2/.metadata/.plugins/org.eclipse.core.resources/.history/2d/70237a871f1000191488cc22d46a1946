package com.inter.project;

public class Reel extends IAffiche implements IOperation{

	private double re;
	private double im;
	
	public Reel(double re, double im) {
		this.re = re;
		this.im = im;
		
	}
	
	@Override
	public String affiche() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public Object plus(Object obj) {
		Complexe com = (Complexe) obj;
		return new Complexe(this.re + com.re, this.im + com.im);
	}

	@Override
	public Object moins(Object obj) {
		Complexe com = (Complexe) obj;
		return new Complexe(this.re - com.re, this.im - com.im);
	}
}