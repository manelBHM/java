  <servlet-mapping>
  	<servlet-name>Facture</servlet-name>
  	<url-pattern>/Facture</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Shopping</servlet-name>
  	<url-pattern>/Shopping</url-pattern>
  </servlet-mapping>
