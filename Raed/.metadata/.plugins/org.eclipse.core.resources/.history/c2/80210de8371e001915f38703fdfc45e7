	private Customer  customer;	
	private Map<Product, Integer>  product;
	
	
	public Customer getCustomer() {
		return customer;
	}
 
	public Map<Product, Integer> getProduct() {
		return product;
	}
 
 
	
 
	
	
	

 
	
	}
