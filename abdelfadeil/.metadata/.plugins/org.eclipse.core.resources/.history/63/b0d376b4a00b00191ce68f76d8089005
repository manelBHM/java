package com.segment;

public class segmentDeDroite {
	
	int point;
	int segmentA;
	int segmentB;
	int longueur;

	public int caculerLongueur() {
		longueur= segmentA - segmentB;
		return longueur;
	}
	
	public void verifierPoint() {
		System.out.println("Pour la commmande :");
		System.out.println("TestSegment "+ segmentA + " "+ segmentB +" "+ point);
	}
}
